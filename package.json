{
  "name": "@jellyfish-dev/ts-client-sdk",
  "version": "0.2.0",
  "description": "Typescript client library for Jellyfish.",
  "license": "Apache-2.0",
  "author": "Membrane Team",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/jellyfish-dev/ts-client-sdk/issues"
  },
  "homepage": "https://github.com/jellyfish-dev/ts-client-sdk#readme",
  "keywords": [
    "webrtc",
    "membrane",
    "jellyfish"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jellyfish-dev/ts-client-sdk.git"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "docs": "typedoc src",
    "format": "prettier --write . --ignore-path ./.eslintignore",
    "format:check": "prettier --check . --ignore-path ./.eslintignore",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "tsc"
  },
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "typed-emitter": "^2.1.0",
    "typedoc": "^0.23.28",
    "typedoc-plugin-external-resolver": "github:ostatni5/typedoc-plugin-external-resolver",
    "typedoc-plugin-mdn-links": "^3.0.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@jellyfish-dev/membrane-webrtc-js": "github:jellyfish-dev/membrane-webrtc-js",
    "events": "^3.3.0",
    "ts-proto": "^1.147.2",
    "typed-emitter": "^2.1.0"
  },
  "directories": {
    "example": "examples"
  }
}
